{
  "name": "zreact",
  "version": "0.0.4",
  "description": "React like,copy by preact",
  "main": "dist/zreact.js",
  "module": "dist/zreact.esm.js",
  "jsnext:main": "dist/zreact.esm.js",
  "minified:main": "dist/zreact.min.js",
  "typings": "src/zreact.ts",
  "repository": "https://github.com/zeromake/zreact",
  "author": "zeromake <a390720046@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeromake/zreact/issues"
  },
  "homepage": "https://github.com/zeromake/zreact",
  "scripts": {
    "clean": "rimraf dist devtools.js devtools.js.map",
    "build:all": "npm run clean && npm run build:dev && npm run build && npm run build:devtools-run",
    "build": "cross-env NODE_ENV=production DEVTOOLS_ENV=production rollup -c config/rollup.config.js",
    "build:devtools-run": "rollup -c config/rollup.config.devtools-run.js",
    "build:dev": "cross-env DEVTOOLS_ENV=production NODE_ENV=dev rollup -c config/rollup.config.js",
    "dev": "cross-env DEVTOOLS_ENV=production NODE_ENV=dev rollup -c config/rollup.config.js -w",
    "test": "babel test/test.jsx -o test/test.js && node test/test.js",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "test:karma:watch": "npm run test:karma -- no-single-run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepublish": "npm run build:all",
    "docs": "rimraf docs && typedoc --exclude src/devtools1.ts src/devtools-run.ts --out docs src"
  },
  "keywords": [
    "zreact",
    "react",
    "virtual dom",
    "vdom",
    "components",
    "virtual",
    "dom"
  ],
  "files": [
    "src",
    "dist",
    "types",
    "devtools.js",
    "devtools.js.map",
    "typings.json",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.1",
    "conventional-changelog-cli": "^1.3.2",
    "cross-env": "^5.0.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.7.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.47.6",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript": "zeromake/rollup-plugin-typescript",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.13.0",
    "ts-loader": "^2.3.1",
    "tslint": "^5.6.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.1",
    "uglify-es": "^3.0.27",
    "webpack": "^3.5.5"
  }
}
